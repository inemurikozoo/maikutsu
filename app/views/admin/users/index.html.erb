<div class="container">
  <h1 class="mt-3 text-center bg-success border border-secondary">会員一覧</h1>
  <div class="col-10-md">
      <div class="table-responsive">
        <table class="table">
          <thead>
          <tr>
            <th>会員ID</th>
            <th>ニックネーム</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
            <th></th>
            <th></th>
            </tr>
          </thead>
          <tbody>
          <tr>
          <td>
            <%= link_to "管理中" %>
          </td>
          <% @users.each do |user| %> 
          <td><%= user.id %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td>ステータス</td>
          <td>退会</td>
          </tr>
          </tbody>
          <% end %>
        </table>
        
      </div>
      
  </div>
</div>